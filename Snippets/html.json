	"Html5 Bakzy structure": {
		"prefix": "B!",
		"body": [
			"<!DOCTYPE html>",
			"<html lang=\"en\">",
			"<head>",
			"    <meta charset=\"UTF-8\">",
			"    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
			"    <title>${1:TITLE}</title>",
			"",
			"    <!--FAVICON-->",
			"    <link rel=\"icon\" type=\"image/png\" href=\"img/favicon.png\">",
			"    <!--///-->",
			"",
			"    <!--CSS-->",
			"    <link rel=\"stylesheet\" href=\"css/styles.css\">",
			"    <!--///-->",
			"    ${2:MORE}",
			"</head>",
			"<body>",
			"    ${0:CODE}",
			"    <!--ANIMA EJERCICIOS BRUNO-->",
			"    <div id=\"wm\">",
			"        <span><img src=\"img/favicon.png\" alt=\"\" width=\"30px\"></span> <p>Ejercicios Anima <br> Bruno Accorenti</p>",
			"    </div>",
			"    <!--///-->",
			"</body>",
			"</html>"
		],
		"description": "Html5 Bakzy structure"
